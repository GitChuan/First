package day01;

import java.util.Scanner;

public class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int new_num = 0;
        while (num != 0) {
            int unit = num % 10;
            num /= 10;
            new_num = new_num * 10 + unit;
        }
        System.out.println(new_num);
    }
}
