package Math;

public class Test {
    public static void main(String[] args) {
        System.out.println(sum(100));
    }

    public static int factorial(int num){
        if(num == 1){
            return 1;
        }

        return num * factorial(num-1);
    }

    public static int sum(int num){
        if(num == 1){
            return 1;
        }

        return num + sum(--num);
    }
}
