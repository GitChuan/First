package Math;

public class Test {
    public static void main(String[] args) {
        System.out.println(findFibonacci(12));
    }

    public static int findFibonacci(int index){
        if(index == 1 || index == 2){
            return 1;
        }

        return findFibonacci(--index) + findFibonacci(--index);
    }

}
